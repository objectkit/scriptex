{
  "name": "com.objectkit.scriptex",
  "version": "1.0.0-b2",
  "description": "A micro library and standalone build tool for the Apple Scripter MIDI-FX plugin",
  "exports": "out/com.objectkit.scriptex/1.0.0-b1/com.objectkit.scriptex.js",
  "scripts": {
    "test": "npm run test/coverage",
    "test/watch": "npm run test/specs -- -w --watch-files src/**/*.*",
    "test/specs": "npm run build && mocha src/test/coffee/**/*Spec.coffee --file tool/mocha/env.coffee --require coffeescript/register",
    "test/coverage": "nyc --nycrc-path tool/istanbuljs/config.json npm run test/specs",
    "build": "npm run build/clean && npm run build/lib && npm run build/doc",
    "build/lib": "rollup -c tool/rollup/build.js",
    "build/doc": "jsdoc -c tool/jsdoc/config.json -d ${npm_package_exports}/../doc/",
    "build/clean": "rm -rf out",
    "test/lint": "eslint -c tool/eslint/config.json src/main/js/**/*.js",
    "postinstall": "npm run test"
  },
  "author": "axel@objectkit.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-multi-entry": "4.0.0",
    "app-module-path": "2.2.0",
    "chai": "4.2.0",
    "coffeescript": "2.5.1",
    "eslint": "7.7.0",
    "jsdoc": "3.6.5",
    "minami": "1.2.3",
    "mocha": "8.1.3",
    "nyc": "15.1.0",
    "rollup": "2.26.8",
    "rollup-plugin-includepaths": "0.2.4",
    "rollup-plugin-terser": "7.0.1",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "uuid": "8.3.0"
  }
}
