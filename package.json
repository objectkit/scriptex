{
  "name": "@objectkit/scriptex",
  "version": "1.0.0-rc.6.2",
  "description": "A metaplugin and microlib for the Scripter MIDI-FX Plugin.",
  "exports": "./out/scriptex.js",
  "type": "module",
  "unpkg": "lib/scriptex.preset.js",
  "files": [
    "lib/scriptex.js",
    "lib/scriptex.preset.js",
    "doc"
  ],
  "keywords": [
    "MIDI",
    "logic pro x",
    "mainstage",
    "scripter",
    "midi-fx",
    "midi-plugin",
    "library",
    "es6"
  ],
  "author": "David Foley <david@objectkit.com>",
  "homepage": "https://objectkit.github.io/scriptex",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/objectkit/scriptex.git"
  },
  "bugs": {
    "url": "https://github.com/objectkit/scriptex/issues"
  },
  "scripts": {
    "pretest": "npm run clean && npm run build/out",
    "test": "npm run test/lint && npm run test/specs",
    "test/specs": "mocha src/test/js/**/*Spec.js --file env/mocha/conf.js",
    "test/lint": "eslint -c env/eslint/conf.json src/main/js/**/*.js",
    "build": "npm run clean && npm run build/out && npm run build/doc",
    "build/out": "rollup -c env/rollup/conf.js --silent",
    "build/doc": "jsdoc -c env/jsdoc/conf.json -d ./doc",
    "clean": "rm -rf out doc lib",
    "prepack": "npm run test && mkdir lib && mv out/* lib/ && rm -rf out lib/scriptex.test.js",
    "postpublish": "npm run clean",
    "install": "npm run test && npm run clean"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "4.0.0",
    "chai": "4.2.0",
    "eslint": "7.10.0",
    "jsdoc": "3.6.6",
    "minami": "1.2.3",
    "mocha": "8.1.3",
    "rollup": "2.28.2",
    "rollup-plugin-includepaths": "0.2.4",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "uuid": "8.3.0"
  },
  "dependencies": {}
}
