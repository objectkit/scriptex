{
  "name": "@objectkit/scriptex",
  "version": "1.0.0-rc4",
  "description": "A microlib and metaplugin for the Scripter MIDI-FX Plugin.",
  "main": "out/scriptex.js",
  "scripts": {
    "test": "npm run test/lint && npm run test/specs",
    "test/watch": "npm run test/specs -- -w --watch-files src/**/*.*",
    "test/specs": "npm run build && mocha src/test/coffee/**/*Spec.coffee --file tool/mocha/env.coffee --require coffeescript/register && rm ./out/scriptex.test.js",
    "test/lint": "eslint -c tool/eslint/config.json src/main/js/**/*.js",
    "build": "npm run build/clean && npm run build/lib && npm run build/doc",
    "build/lib": "rollup -c tool/rollup/build.js",
    "build/doc": "jsdoc -c tool/jsdoc/config.json -d ${npm_package_main}/../doc/",
    "build/clean": "rm -rf out",
    "postinstall": "npm run test"
  },
  "author": "David Foley <david@objectkit.com>",
  "homepage": "https://objectkit.github.io/scriptex",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/objectkit/scriptex.git"
  },
  "bugs": {
    "url": "https://github.com/objectkit/scriptex/issues"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "4.0.0",
    "app-module-path": "2.2.0",
    "chai": "4.2.0",
    "coffeescript": "2.5.1",
    "eslint": "7.10.0",
    "jsdoc": "3.6.6",
    "minami": "1.2.3",
    "mocha": "8.1.3",
    "rollup": "2.28.2",
    "rollup-plugin-includepaths": "0.2.4",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "uuid": "8.3.0"
  }
}
